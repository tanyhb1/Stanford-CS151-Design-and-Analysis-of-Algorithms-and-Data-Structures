import csv

def main():
    results = [9,7,6,5,4,3,2,8,1]

    x = Counter()
    print(sort(results, x))
    print(x.count)
def sort(x, counter):
    if len(x) == 1:
        return(x)
    else:
        middle = len(x)/2
        left = sort(x[:int(middle)], counter)
        right = sort(x[int(middle):], counter)
        return(merge(left, right, counter))
        
def merge(left, right, x):
    i, j = 0, 0
    c = []

    while(len(c) < (len(left) + len(right)) ):
        if left[i] < right[j]:
            c.append(left[i])
            i = i + 1
        else:
            c.append(right[j])
            x.count = x.count+(len(left)-i)
            j = j + 1
        if i == len(left):
            c.extend(right[j:])
            break
        elif j == len(right):
            c.extend(left[i:])
            break
    return(c)
class Counter(object):
    count = 0
    def counter(self):
        self.count = self.count + 1

    
if __name__ == '__main__':
    main()
    